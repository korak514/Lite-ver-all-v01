<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <!--Background Extra Colors-->

    <Color x:Key="exColor1">#060501</Color>
    <Color x:Key="exColor3">#2F2707</Color>
    <Color x:Key="exColor2">#16640D</Color>

    <!--Background Colors-->
    <Color x:Key="primaryBackColor1">#0D1500</Color>

    <Color x:Key="primaryBackColor2">#0D2500</Color>

    <Color x:Key="primaryBackColor3">#085D0E</Color>
    <Color x:Key="primaryBackColor4">#07780B</Color>

